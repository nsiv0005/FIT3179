{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Migrant Arrivals, Departures, and Net Overseas Migration",
    "data": {
      "url": "https://raw.githubusercontent.com/nsiv0005/FIT3179/refs/heads/main/Assignment%202/data/Graph_1.1-Overseas%20migration.csv",  
      "format": {
        "type": "csv"
      }
    },
    "width": 700,
    "height": 400,
    "mark": "line",
    "encoding": {
      "x": {
        "field": "Date",
        "type": "temporal",
        "title": "Date",
        "axis": {"format": "%b-%y"}
      },
      "y": {
        "field": "value",
        "type": "quantitative",
        "title": "Population ('000)"
      },
      "color": {
        "field": "category",
        "type": "nominal",
        "title": "Migration Category"
      }
    },
    "transform": [
      {
        "fold": [
          "Migrant arrivals(000)", 
          "Migrant departures(000)", 
          "Net overseas migration(000)"
        ],
        "as": ["category", "value"]
      }
    ]
  }
  




  
  